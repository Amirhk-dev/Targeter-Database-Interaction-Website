<%- include("../header") -%>
    <div class="row">
        <div class="col-lg-12">
            <form method="GET" action='finishCreatingSubframeEntry'>
                <p class="lead_text">The Subframe ID is Created in the Database.</p>
                <div class="form-group">
                    <label class="label-text" for="facility_name_select">
                        Facility Name
                    </label>
                    <input type="text" name="facility_name" class="form-control"
                    id="facility_name" value=<%= facility_code_name %> disabled>
                </div>
                <div class="form-group">
                    <label class="label-text" for="subframe_type_select">
                        Subframe Type Name
                    </label>
                    <input type="text" name="subframe_type" class="form-control"
                    id="subframe_type" value=<%= subframe_type_name %> disabled>
                </div>
                <div class="form-group">
                    <label class="label-text">Serial Number</label>
                    <input type="text" name="serial_number" class="form-control"
                    id="serial_number" value=<%= serial_number %> disabled>
                </div>
                <div class="form-group">
                    <label class="label-text" for="subframe_id">
                        The "SubframeID" is:
                    </label>
                    <input type="text" name="subframe_id" class="form-control"
                    id="subframe_id" value=<%= final_id %> disabled>
                </div>
                <button class="btn btn-secondary">Go to the Main Page</button>
            </form>
        </div>
    </div>
<%- include("../footer") -%>