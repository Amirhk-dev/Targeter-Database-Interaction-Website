<%- include("../header") -%>

<form method="POST" action='/get_subframe_ids'>
    <div class="row">
        <button class="btn btn-secondary" data-bs-toggle="collapse"
            data-bs-target="#get_subframe_ids" aria-expanded="false"
            aria-controls="collapseExample" style="margin-top: 20px;">
                Get Subframe IDs
        </button>
    </div>
</form>

<% if (Subframe_IDs.length != 0) { %>
    <div class="row">
        <div class="collapse" id="get_subframe_ids" style="display: block;">
            <table class="table">
                <tbody>
                    <% for(var idx=0; idx < Subframe_IDs.length; idx++) { %>
                        <tr>
                            <td>
                                <%= Subframe_IDs[idx] %>
                            </td>
                        </tr>
                    <% } %>
                </tbody>
            </table>
        </div>
    </div>
<% } %>

<form method="POST" action='/get_subframe_information'>
    <div class="row">
        <button class="btn btn-secondary" data-bs-toggle="collapse"
            data-bs-target="#get_subframe_information" aria-expanded="false"
            aria-controls="collapseExample" style="margin-bottom: 5px;">
                Get Subframe Information
        </button>
        <div class="form-group">
            <input type="text" name="subframe_id" class="form-control"
            placeholder="Enter 14 Characters Subframe ID"
            minlength="14" maxlength="14">
        </div>
    </div>
</form>

<form method="POST" action='/get_roi_information'>
    <div class="row">
        <button class="btn btn-secondary" data-bs-toggle="collapse"
