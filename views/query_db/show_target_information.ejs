<%- include("../header") -%>

<div class="row twopanel">
    <div class="col-lg-12">
        <h1 class="site_msg" style="text-align: center;">
        <em>Target(s) lie in the Sample.</em>
        <br>
        <br>
        <div class="row">
            <img class="center" src='target.jpg' alt="ROI Photo">
        </div>
        <br>
        <br>
            Information for the Subframe <%= subframe_id %> and ROI <%= roi_id %> and Sample <%= sample_id %> and Target <%= target_id %>
        </h1>
    </div>
</div>


<div class="row twopanel">
    <div class="row">
        <table class="table">
            <tbody>
                <% for(var idx=0; idx < Object.keys(selected_target).length; idx++) { %>
                    <tr>
                        <th width="50%">
                            <%= Object.keys(selected_target)[idx] %>
                        </th>
                        <% if (Object.values(selected_target)[idx]) { %>
                        <td width="50%">
                            <%= Object.values(selected_target)[idx] %>
                        </td>
                        <% } else{ %>
                        <td width="50%">
                            -
                        </td>
                        <% } %>    
                    </tr>
                <% } %>
            </tbody>
        </table>
    </div>
</div>

<% if (typeof all_subframe_information['target_events'] !== 'undefined') { %>
    <% if (all_subframe_information['target_events'].length > 0) { %>
        <div class="row twopanel">
            <div class="col-lg-12">
                <h1 class="site_msg" style="text-align: center;">
                    Events
                </h1>
            </div>
        </div>

        <div class="row twopanel">
            <% for(var idx=0; idx < all_subframe_information['target_events'].length; idx++) { %>
                <div class="row">
                    <button class="btn btn-secondary" data-bs-toggle="collapse"
                        data-bs-target="#collapse_event<%= idx %>" aria-expanded="false"
                        aria-controls="collapseExample" style="margin-top: 20px;">
                            Event <%= idx+1 %>
                    </button>
                </div>

                <div class="collapse" id="collapse_event<%= idx %>">
                    <div class="row">
                        <table class="table">
                            <tbody>
                                <% for(var i=0; i < Object.keys(all_subframe_information['target_events'][idx]).length; i++) { %>
                                    <tr>
                                        <th width="50%">
                                            <%= Object.keys(all_subframe_information['target_events'][idx])[i] %>
                                        </th>
                                        <% if (Object.keys(all_subframe_information['target_events'][idx])[i]) { %>
                                        <td width="50%">
                                            <%= Object.values(all_subframe_information['target_events'][idx])[i] %>
                                        </td>
                                        <% } else{ %>
                                        <td width="50%">
                                            -
                                        </td>
                                        <% } %>    
                                    </tr>
                                <% } %>
                            </tbody>
                        </table>
                    </div>
                    
                    <% if (typeof all_subframe_information['target_fiducials'] !== 'undefined') { %>
                        <% if (all_subframe_information['target_fiducials'].length > 0) { %>
                            <div class="row twopanel">
                                <div class="col-lg-12">
                                    <h1 class="site_msg" style="text-align: center;">
                                        Fiducials
                                    </h1>
                                </div>
                            </div>
                    
                            <div class="row twopanel">
                                <% for(var iter=3*idx; iter < 3*idx+3; iter++) { %>    
                                    <div class="row">
                                        <button class="btn btn-outline-secondary btn-sm" data-bs-toggle="collapse"
                                            data-bs-target="#collapse_fiducial<%= iter %>" aria-expanded="false"
                                            aria-controls="collapseExample" style="margin-top: 20px;">
                                            Fiducial <%= iter%3+1 %>
                                        </button>
                                    </div>                
                                    <div class="collapse" id="collapse_fiducial<%= iter %>">
                                        <div class="row">
                                            <table class="table">
                                                <tbody>
                                                    <% for(var i=0; i < Object.keys(all_subframe_information['target_fiducials'][iter][0]).length; i++) { %>
                                                        <tr>
                                                            <th width="50%">
                                                                <%= Object.keys(all_subframe_information['target_fiducials'][iter][0])[i] %>
                                                            </th>
                                                            <% if (Object.keys(all_subframe_information['target_fiducials'][iter][0])[i]) { %>
                                                            <td width="50%">
                                                                <%= Object.values(all_subframe_information['target_fiducials'][iter][0])[i] %>
                                                            </td>
                                                            <% } else{ %>
                                                            <td width="50%">
                                                                -
                                                            </td>
                                                            <% } %>    
                                                        </tr>
                                                    <% } %>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                <% } %>
                            </div>
                        <% } %>
                    <% } %>
                </div>
            <% } %>
        </div>
    <% } %>
<% } %>

<form method="GET" id="query_finished" action='/'> 
    <div class="row twopanel">
        <button type="submit" form="query_finished" class="btn btn-outline-primary btn-block">
            Done (Go to Home Page)
        </button>
    </div>
</form>

<%- include("../footer") -%>