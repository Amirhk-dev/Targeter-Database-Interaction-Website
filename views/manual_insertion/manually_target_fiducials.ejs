                                <input class="form-control" type="time" id="target_event_fid3_experiment_time"
                                    name="target_event_fid3_experiment_time">
                            </div>
                        </div>
                </div>
            </div>
            <button class="btn btn-secondary" type="submit" form="enterTargetData">Enter the Target Information</button>
        </form>
    </div>
</div>
<%- include("footer") -%>
